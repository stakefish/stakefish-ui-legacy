{
  "name": "stakefish-ui",
  "version": "0.0.1-alpha",
  "private": true,
  "author": "stakefish team",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "pre-push": {
    "run": [
      "lint",
      "format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.0-alpha.14",
    "@storybook/addon-essentials": "^6.3.0-alpha.14",
    "@storybook/addon-links": "^6.3.0-alpha.14",
    "@storybook/node-logger": "^6.3.0-alpha.14",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.0-alpha.14",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "eslint": "^7.25.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "babel-loader": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
